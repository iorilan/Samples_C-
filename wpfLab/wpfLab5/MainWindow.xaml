<Window x:Class="wpfLab5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
  
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary1.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <StackPanel>
        <Image Width="100" Height="30" Source="/wpfLab5;component/Images/hills.jpg"></Image>
        <Image Width="100" Height="30" x:Name="img2"></Image>
        <Image Width="100" Height="30" x:Name="img3"></Image>
        <Button Background="{StaticResource ResourceKey=redBrush}" Width="30" Height="24" Content="A"></Button>
        <Button Background="{StaticResource ResourceKey=yellowBrush}" Width="30" Height="24" Content="B"></Button>
    </StackPanel>
</Window>
